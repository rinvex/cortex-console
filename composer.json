{
    "name": "cortex/console",
    "description": "Cortex Console is a set of powerful tools for administrators and system support staff, to maintain the project through a web console.",
    "type": "cortex-module",
    "keywords": [
        "cortex",
        "console",
        "maintenance",
        "administration",
        "laravel",
        "system",
        "rinvex"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-console/issues",
        "source": "https://github.com/rinvex/cortex-console",
        "docs": "https://github.com/rinvex/cortex-console/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-console/contributors"
        }
    ],
    "require": {
        "php": "^7.4.0 || ^8.0.0",
        "ext-json": "*",
        "composer/composer": "^2.0.0",
        "cortex/foundation": "^6.0.0",
        "diglactic/laravel-breadcrumbs": "^7.0.0",
        "illuminate/console": "^8.0.0 || ^9.0.0",
        "illuminate/database": "^8.0.0 || ^9.0.0",
        "illuminate/filesystem": "^8.0.0 || ^9.0.0",
        "illuminate/http": "^8.0.0 || ^9.0.0",
        "illuminate/support": "^8.0.0 || ^9.0.0",
        "laravel/framework": "^8.0.0 || ^9.0.0",
        "laravelcollective/html": "^6.2.0",
        "proengsoft/laravel-jsvalidation": "^4.4.0",
        "rinvex/laravel-composer": "^5.0.0",
        "rinvex/laravel-menus": "^5.0.0",
        "rinvex/laravel-support": "^5.0.0",
        "symfony/console": "^5.1.4",
        "yajra/laravel-datatables-buttons": "^4.10.0",
        "yajra/laravel-datatables-fractal": "^1.6.0",
        "yajra/laravel-datatables-html": "^4.36.0",
        "yajra/laravel-datatables-oracle": "^9.15.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.30.0",
        "phpunit/phpunit": "^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "Cortex\\Console\\": "src/",
            "Cortex\\Console\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortex\\Console\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cortex\\Console\\Providers\\ConsoleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
