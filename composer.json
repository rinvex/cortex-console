{
    "name": "cortex/console",
    "type": "cortex-module",
    "description": "Cortex Console is a set of powerful tools for administrators and system support staff, to maintain the project through a web console.",
    "keywords": [
        "cortex",
        "console",
        "maintenance",
        "administration",
        "laravel",
        "system",
        "rinvex"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-console/issues",
        "source": "https://github.com/rinvex/cortex-console",
        "docs": "https://github.com/rinvex/cortex-console/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-console/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "composer/composer": "^1.5.0",
        "cortex/foundation": "dev-develop",
        "illuminate/console": "~5.5.0|~5.6.0",
        "illuminate/database": "~5.5.0|~5.6.0",
        "illuminate/filesystem": "~5.5.0|~5.6.0",
        "illuminate/http": "~5.5.0|~5.6.0",
        "illuminate/support": "~5.5.0|~5.6.0",
        "laravel/framework": "~5.5.0|~5.6.0",
        "laravelcollective/html": "~5.5.0|~5.6.0",
        "proengsoft/laravel-jsvalidation": "^2.1.0",
        "rinvex/composer": "dev-develop",
        "rinvex/fort": "dev-develop",
        "rinvex/menus": "dev-develop",
        "symfony/console": "~2.8|~3.0",
        "yajra/laravel-datatables-buttons": "^3.0.0",
        "yajra/laravel-datatables-fractal": "^1.0.0",
        "yajra/laravel-datatables-html": "^3.0.0",
        "yajra/laravel-datatables-oracle": "^8.0.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.4.0",
        "phpunit/phpunit": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "Cortex\\Console\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cortex\\Console\\Providers\\ConsoleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
