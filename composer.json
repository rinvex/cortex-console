{
    "name": "cortex/console",
    "description": "Cortex Console is a set of powerful tools for administrators and system support staff, to maintain the project through a web console.",
    "type": "cortex-module",
    "keywords": [
        "cortex",
        "console",
        "maintenance",
        "administration",
        "laravel",
        "system",
        "rinvex"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-console/issues",
        "source": "https://github.com/rinvex/cortex-console",
        "docs": "https://github.com/rinvex/cortex-console/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-console/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "composer/composer": "^1.5.0",
        "cortex/foundation": "^1.0.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.1.0",
        "illuminate/console": "~5.7.0",
        "illuminate/database": "~5.7.0",
        "illuminate/filesystem": "~5.7.0",
        "illuminate/http": "~5.7.0",
        "illuminate/support": "~5.7.0",
        "laravel/framework": "~5.7.0",
        "laravelcollective/html": "~5.7.0",
        "proengsoft/laravel-jsvalidation": "^2.3.0",
        "rinvex/laravel-composer": "^1.0.0",
        "rinvex/laravel-menus": "^1.0.0",
        "silber/bouncer": "v1.0.0-rc.3",
        "symfony/console": "^4.1.0",
        "yajra/laravel-datatables-buttons": "^4.2.0",
        "yajra/laravel-datatables-fractal": "^1.2.0",
        "yajra/laravel-datatables-html": "^3.8.0",
        "yajra/laravel-datatables-oracle": "^8.8.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.22.0",
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Cortex\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortex\\Console\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cortex\\Console\\Providers\\ConsoleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
